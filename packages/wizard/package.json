{
  "name": "@tecsinapse/wizard",
  "version": "3.7.8-alpha.0",
  "description": "TecSinapse Wizard",
  "keywords": [
    "material-ui",
    "react",
    "tecsinapse",
    "ui-kit",
    "wizard"
  ],
  "homepage": "https://tecsinapse.github.io/ui-kit/",
  "bugs": {
    "url": "https://github.com/tecsinapse/ui-kit/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/tecsinapse/ui-kit.git"
  },
  "license": "MIT",
  "author": {
    "name": "TecSinapse"
  },
  "sideEffects": false,
  "main": "build/index.js",
  "typings": "build/index.d.ts",
  "files": [
    "build",
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
