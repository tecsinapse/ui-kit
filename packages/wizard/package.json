      "git add"
    ]
  },
  "dependencies": {
    "@tecsinapse/ui-kit": "^5.7.8-alpha.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  }
}
