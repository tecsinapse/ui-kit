{
  "name": "@tecsinapse/ui-kit",
  "description": "TecSinapse UI-KIT",
  "version": "0.0.45",
  "browser": "dist/index.js",
  "main": "dist/index.cjs.js",
  "module": "build/index.js",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@emotion/cache": "^10.0.0",
    "@emotion/core": "^10.0.6",
    "@emotion/styled": "^10.0.6",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/lab": "^3.0.0-alpha.30",
    "@material-ui/styles": "^3.0.0-alpha.10",
    "@mdi/js": "^3.0.39",
    "@mdi/react": "^1.1.0",
    "@storybook/addon-actions": "^4.0.2",
    "@storybook/addon-info": "^4.0.4",
    "@storybook/addon-knobs": "^4.0.4",
    "@storybook/addon-options": "^4.0.4",
    "@storybook/addon-storysource": "^4.0.4",
    "@storybook/react": "^4.0.2",
    "@tecsinapse/es-utils": "^0.0.92",
    "@tecsinapse/eslint-config-tecsinapse-js": "^2.1.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-inline-react-svg": "^1.0.1",
    "classnames": "^2.2.6",
    "docz": "^0.13.7",
    "docz-theme-default": "^0.13.7",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.3",
    "material-ui-search-bar": "1.0.0-beta.13",
    "prop-types": "^15.6.2",
    "react": "16.7.0-alpha.2",
    "react-dom": "16.7.0-alpha.2",
    "react-select": "^2.3.0",
    "react-test-renderer": "^16.6.0",
    "react-virtualized": "^9.21.0",
    "react-window": "^1.5.1",
    "regenerator-runtime": "^0.12.1",
    "rollup": "^0.66.6",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-babel": "4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-filesize": "^5.0.1",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "storybook-addon-smart-knobs": "^3.3.1",
    "typeface-roboto": "^0.0.54"
  },
  "files": [
    "build",
    "dist"
  ],
  "scripts": {
    "build": "rm -rf build/ && babel src/ -d build/ && rollup -c",
    "build-all": "yarn build && yarn docz:build && yarn storybook:build",
    "build-publish": "yarn build-all && npm publish",
    "dev": "rollup -c -w",
    "docz:build": "docz build -d docs/docz/ --base='/ui-kit/docz/' ",
    "docz:dev": "docz dev",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o docs/storybook/",
    "test": "NODE_ENV=test jest test/"
  },
  "husky": {
    "hooks": {
      "post-commit": "git update-index --again",
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "moduleDirectories": [
      "bower_components",
      "node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/jest-setup.js",
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --max-warnings=0 --fix",
      "prettier --write",
      "git add"
    ]
  },
  "peerDependencies": {
    "@material-ui/core": "^3.9.1",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/styles": "^3.0.0-alpha.10",
    "@mdi/js": "^3.0.39",
    "@mdi/react": "^1.1.0",
    "@tecsinapse/es-utils": "0.0.92",
    "classnames": "^2.2.6",
    "material-ui-search-bar": "1.0.0-beta.13",
    "memoize-one": "^5.0.0",
    "react": ">= 16.3.x",
    "react-select": "^2.3.0",
    "react-virtualized": "^9.21.0",
    "react-window": "^1.5.1",
    "typeface-roboto": "^0.0.54"
  },
  "sideEffects": false
}
