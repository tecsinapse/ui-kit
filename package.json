  "files": [
    "build",
    "dist"
  ],
  "scripts": {
    "build": "rm -rf build/ && babel src/ -d build/ && rollup -c",
    "build-all": "yarn build && yarn docz:build && yarn storybook:build",
    "build-publish": "yarn build-all && npm publish",
    "dev": "rollup -c -w",
    "docz:build": "docz build -d docs/docz/ --base='/ui-kit/docz/' ",
    "docz:dev": "docz dev",
    "storybook": "start-storybook -p 9001 -c .storybook -s ./storybook-public",
    "storybook:build": "build-storybook -c .storybook -o docs/storybook/",
    "test": "NODE_ENV=test jest test/"
  },
  "husky": {
    "hooks": {
      "post-commit": "git update-index --again",
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "moduleDirectories": [
      "bower_components",
      "node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/jest-setup.js",
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --max-warnings=0 --fix --no-ignore",
      "prettier --write",
      "git add"
    ]
  },
  "peerDependencies": {
