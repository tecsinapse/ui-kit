{
  "name": "@tecsinapse/ui-kit",
  "private": true,
  "description": "TecSinapse UI-KIT",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test": "NODE_ENV=test jest",
    "test:windows": "set NODE_ENV=test&& jest",
    "coverage": "jest --coverage",
    "bootstrap": "lerna bootstrap --use-workspaces",
    "prebuild": "rimraf -rf build/",
    "build": "lerna exec --parallel -- babel --root-mode upward src -d build --ignore **/*.story.js,**/*.spec.js,**/*.story.mdx",
    "build:storybook": "build-storybook -c .storybook -s ./storybook-public -o docs/",
    "dev": "start-storybook -p 5555",
    "eslint:fix": "npx eslint ./packages/carousel/src ./packages/pickers/src ./packages/wizard/src ./packages/uploader/src ./packages/ui-kit/src/ ./packages/table/src/ --ext .js --fix",
    "prepublish": "yarn build",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --max-warnings=0 --fix --ext js --no-ignore",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.2.1",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.2.1",
    "@mdi/js": "^3.0.39",
    "@mdi/react": "^1.1.0",
    "@tecsinapse/es-utils": "^6.0.10",
    "clsx": "^1.0.4",
    "immutable": "^4.0.0-rc.12",
    "material-ui-search-bar": "1.0.0-beta.13",
    "prop-types": "^15.6.2",
    "react-select": "^2.3.0",
    "react-sizeme": "^2.5.2",
    "react-text-mask": "^5.4.3",
    "react-virtualized": "^9.21.0",
    "react-window": "^1.5.1",
    "text-mask-addons": "^3.8.0",
    "text-mask-core": "^5.1.2"
  },
  "devDependencies": {
    "coveralls": "^3.1.0",
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/plugin-transform-react-jsx": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-docs": "^5.3.12",
    "@storybook/addon-options": "^5.3.12",
    "@storybook/react": "^5.3.12",
    "@storybook/theming": "^5.3.19",
    "@tecsinapse/eslint-config-tecsinapse-js": "^3.0.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "babel-core": "^7.0.0-beta.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-inline-react-svg": "^1.0.1",
    "core-js": "2.5.7",
    "danger": "^10.2.0",
    "eslint-config-react-app": "^5.0.2",
    "eslint-plugin-mdx": "^1.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react-hooks": "^2.3.0",
    "git-dirty": "^1.0.2",
    "graphql-hooks": "^3.4.0",
    "husky": "^1.3.1",
    "jest": "^24.9.0",
    "lerna": "^3.22.1",
    "lint-staged": "^8.1.3",
    "markdown-to-jsx": "^6.10.3",
    "npm-check-updates": "^3.1.23",
    "prettier": "^2.0.5",
    "prettier-plugin-packagejson": "^2.2.5",
    "promise-polyfill": "^8.1.3",
    "react-popper": "^1.3.4",
    "react-textarea-autosize": "^7.1.0",
    "regenerator-runtime": "^0.12.1",
    "rimraf": "^2.6.3",
    "rollup": "^0.68.2",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-babel": "4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-filesize": "^5.0.1",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "source-map-explorer": "^2.0.0"
  },
  "engines": {
    "node": ">=14"
  }
}
